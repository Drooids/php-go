{
    "alias": "go.phar",
    "chmod": "0755",
    "directories": ["src/lib"],
    "files-bin": ["composer.json"],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "phpunit",
                "phpunit-test-case",
                "Tests",
                "tests",
                "yaml"
            ],
            "in": "src/vendors"
        }
    ],
    "git-version": "git_version",
    "main": "bin/go",
    "output": "go.phar",
    "replacements": {
        "manifest_url": "http://herrera-io.github.com/php-go/manifest.json"
    },
    "stub": true
}